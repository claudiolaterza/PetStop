<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.LocalPetFriendlyActivity">

    <TextView
        android:id="@+id/tv_nome_perfil_petfriendly"
        android:layout_width="369dp"
        android:layout_height="61dp"
        android:layout_marginBottom="264dp"
        android:hint="Nome do PetFriendly"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_local_perfil_petfriendly"
        android:layout_width="363dp"
        android:layout_height="69dp"
        android:layout_marginTop="48dp"
        android:hint="Endereço da PetFriendly"
        android:shadowColor="@color/black"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nome_perfil_petfriendly" />

    <TextView
        android:id="@+id/tv_latitude_perfil_petfriendly"
        android:layout_width="366dp"
        android:layout_height="67dp"
        android:hint="Latitude"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_longitude_perfil_petfriendly" />

    <TextView
        android:id="@+id/tv_longitude_perfil_petfriendly"
        android:layout_width="366dp"
        android:layout_height="67dp"
        android:layout_marginTop="120dp"
        android:hint="Longitude"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nome_perfil_petfriendly" />

    <ImageView
        android:id="@+id/voltar_perfil_petfriendly"
        android:layout_width="69dp"
        android:layout_height="33dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.046"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/voltar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="Nome"
        android:textColor="#939090"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_nome_perfil_petfriendly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Endereço"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/tv_local_perfil_petfriendly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.127"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_nome_perfil_petfriendly"
        app:layout_constraintVertical_bias="0.611" />

</androidx.constraintlayout.widget.ConstraintLayout>